<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle" >
            <size android:height="6dp" android:width="12dp"/>
            <gradient android:startColor="#66000000" android:endColor="#66000000" android:angle="90"/>
            <corners android:topLeftRadius="200dp" android:topRightRadius="200dp"/>
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <size android:height="5dp" android:width="10dp"/>
            <solid android:color="@color/colorPrimary"/>
            <corners android:topLeftRadius="200dp" android:topRightRadius="200dp"/>
        </shape>
    </item>

</layer-list>